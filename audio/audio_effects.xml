	<preprocess>
		<stream type="mic">
			<apply effect="aec1">
			<param id="0" name="PARAM_UL" value=
                "/*----------------------------------------------*/
                /*Mic order*/
                4, 1, 3, 2, 0, 0, 0, 0, /* [Mic1, Mic2, Mic3...], 0 or not fit: default */
                /*----------------------------------------------*/
                /*Gain*/
                0, /*dB*/
                /*----------------------------------------------*/
                /*filter*/
                1, 50, 0, 0,  /*[type0, fc0, type1, fc1], type: 0:off, 1:HPF, 2:LPF*/
                /*----------------------------------------------*/
                /*AEC*/
                1, 45,        /*[Enable, delayTime]*/
                /*AGC*/
                1, /*[Enable]*/
                -2, 10, -20, /*[target, maxGain, minGain], unit: dB*/
                /*Mic array*/
                1,             /*ED: [Enable]*/
                1, 1, 1, 1, 0, /*BF: [Enable, NREnable, enhEnable, endfireEnable, FESel]*/
                4500, 60000, 120000, 700, 700, /*BF: [micDist, angLeftTd, angRightTd, angLeftBin, angRightBin] * 1000*/
                6450, 0, 0, 60000, -60000,     /*endfire: [MicDist, AngLeftBin, AngRightBin, AngRightTd, AngLeftTd] * 1000*/
                /*----------------------------------------------*/
                /*PEQ*/
                0, 7, /*[Enable, peqBands]*/
                1000, 8000, 3000, 4000, 5000, 6000, 7000, /*fc*/
                707, 707, 707, 707, 707, 707, 707, /*Q * 1000*/
                0, -10, 0, 0, 0, 0, 0, /*G*/
                0, 2, 0, 0, 0, 0, 0, /*Type: [0:off, 1:LPF, 2:HPF, 3:Peak, 4:Lowshelf, 5:Highshelf]*/
                /*MBC*/
                0, 3,     /*[Enable, mbcBands:  3]*/
                100, 200, /*fc*/
                0, 0,  0, 0,  0, 0, /*X*/
                0, 0,  0, 0,  0, 0, /*K * 1000*/
                0, 0, 0, /*G*/
                50, 50, 50,    /*Attack * 1000*/
                100, 100, 100, /*Release * 1000*/
                /*Limiter*/
                1,    /*[Enable]*/
                0, -2, /*[Gain, Threshold], unit:dB*/
                /*----------------------------------------------*/
                /*logcat*/
                1, 0 /*[logcat, audio_grab]*/"/>
			</apply>
		</stream>
	</preprocess>
	<postprocess>
		<stream type="default">
			<apply effect="antdownfx">
				<param id="1" name="PARAM_DL" value=
                "/*----------------------------------------------*/
                /*Gain*/
                0, /*dB*/
                /*----------------------------------------------*/
                /*Ref channel*/
                0, /* =0: mix all channel, 1: channel 1, 2: channel 2 ...*/
                0, /* downlink ahead ref offset, unit: ms*/
                /*----------------------------------------------*/
                /*filter*/
                1, 100, 0, 0, //[type0, fc0, type1, fc1]*/
                /*----------------------------------------------*/
                /*PEQ*/
                0, 7, /*[Enable, peqBands]*/
                100, 200, 3000, 400, 500, 600, 700, /*fc*/
                707, 707, 707, 707, 707, 707, 707,  /*Q * 1000*/
                0, 0, 6, 0, 0, 0, 0, /*G*/
                0, 0, 3, 0, 0, 0, 0, /*Type: [=0:off, 1:LPF, 2:HPF, 3:Peak, 4:Lowshelf, 5:Highshelf]*/
                /*MBC*/
                0, 1,     /*[Enable, mbcBands: = 3]*/
                3000, 6000, /*fc*/
                0, 0,  0, 0,  0, 0, /*X*/
                0, 0,  0, 0,  0, 0, /*K * 1000*/
                0, 0, 0, /*G*/
                50, 50, 50,    /*Attack * 1000*/
                100, 100, 100, /*Release * 1000*/
                /*Limiter*/
                1,     /*[Enable]*/
                0, -2, /*[Gain, Threshold], unit:dB*/
                /*----------------------------------------------*/
                /*logcat*/
                1, 0 /*[logcat, audio_grab]*/"/>
			</apply>
		</stream>
	</postprocess>
